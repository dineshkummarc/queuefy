{
    "name": "rakshitbharat/queuefy",
    "description": "This package will take care of Queue from Cron Job it self.",
    "keywords": [
        "rakshitbharat",
        "queuefy"
    ],
    "homepage": "https://github.com/rakshitbharat/queuefy",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rakshit Patel",
            "email": "rakshitbharatproject@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "6.x"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rakshitbharat\\Queuefy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rakshitbharat\\Queuefy\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rakshitbharat\\Queuefy\\QueuefyServiceProvider"
            ],
            "aliases": {
                "Queuefy": "Rakshitbharat\\Queuefy\\QueuefyFacade"
            }
        }
    }
}
